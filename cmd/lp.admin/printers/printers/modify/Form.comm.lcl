#F3
name=Parity:
lininfo="comm.lcl:F3"
nrow=3
ncol=1
frow=3
fcol=40
rows=1
columns=6
rmenu={ odd even none }
value=${($datafile)parity:-none}
#	if fmlgrep parodd=no $datafile > /dev/null;
#	then
#		if fmlgrep parenb=no $datafile > /dev/null;
#		then
#			echo none;
#		else
#			echo even;
#		fi;
#	else
#		echo odd;
#	fi
#`
valid=`
	regex -v "$F3"
	'^odd$'	'parodd parenb'
	'^even$'	'-parodd parenb'
	'^none$'	'-parodd -parenb'
	'^.*$'	'fail'
	| set -l lp_errtmp;
	if [ "$lp_errtmp" != 'fail' ];
	then
	    echo true;
	else
	    echo false;
	fi;
	set -l lp_F3="$lp_errtmp";
`
invalidmsg='Invalid entry:  please enter one of the CHOICES.'

