#ident	"@(#)lp.admin:printers/printers/modify/Form.comm.lcl	1.4.3.2"

form='Printer Communication Setup Subtask'

help=OPEN TEXT $INTFBASE/Text.itemhelp $LININFO 
framemsg="Press \[SAVE\] after completing the form."

done=`
    message -w "Saving Printer communication setup information, please wait.";
	if [ -n "$TFADMIN" ]; then $TFADMIN lpadmin -p $lp_printer -o "stty='$F2 $lp_F3 $lp_F4 $lp_F5 $lp_F6 $lp_F7 $lp_F8 $lp_F9 $lp_F10 $lp_F11 $lp_F12 $lp_F13 $lp_F14 $lp_F15 $lp_F16 $lp_F17 $lp_F18'" > /dev/null 2> $error;
	else lpadmin -p $lp_printer -o "stty='$F2 $lp_F3 $lp_F4 $lp_F5 $lp_F6 $lp_F7 $lp_F8 $lp_F9 $lp_F10 $lp_F11 $lp_F12 $lp_F13 $lp_F14 $lp_F15 $lp_F16 $lp_F17 $lp_F18'" > /dev/null 2> $error; fi;
	if [ "$RET" != "0" -a -s $error ];
	then
	    echo "OPEN TEXT $OAMBASE/add-ons/lp/printers/printers/modify/Text.comm";
	else
	    cosend lpdata "get_printer/n$lp_printer" > /dev/null 2>/dev/null;
	    if [ -n "$TFADMIN" ]; then $TFADMIN lpstat -p $lp_printer -l > /usr/tmp/stat$$;
	    else lpstat -p $lp_printer -l > /tmp/stat$$; fi;
    	    cat /tmp/stat$$ | sed 's/: /=/gp' | 
	    sed 's/^	//gp' | sed 's/After fault/recover/gp' |
	    sed 's/Content types/content/gp'  >> $datafile;
	    rm -f /tmp/stat$$;
	    cat /etc/lp/printers/$lp_printer/configuration | 
	    sed 's/: /=/gp' >> $datafile;
	    message -w "Printer communication setup successful - Printer modify complete.";
	    rm -f $error;
	fi;
`close $OAMBASE/add-ons/lp/printers/printers/modify/Form.comm.lcl

close=`
	codestroy -R printers.add.comm lpdata;
	unset	-l lp_valtmp
		-l lp_errtmp;
`

