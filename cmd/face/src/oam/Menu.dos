#ident	"@(#)face:src/oam/Menu.dos	1.2"

menu=Global MS-DOS Program Administration

framemsg="Move the cursor to the item you want and press ENTER to select it."

help=OPEN TEXT $INTFBASE/Text.oamhelp $!LININFO 

lifetime=permanent



name=add
description=Add Global MS-DOS Programs
lininfo="/usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/add \"Add Global MS-DOS Programs\"" add
action=OPEN FORM $INTFBASE/Form.interface /usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/add Form.add


name=list
