lininfo="/usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/list \"List Global MS-DOS Programs\"" list
action=OPEN MENU $INTFBASE/Menu.interface /usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/list Menu.list


name=modify
description=Modify Global MS-DOS Programs
lininfo="/usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/modify \"Modify Global MS-DOS Programs\"" modify
action=OPEN MENU $INTFBASE/Menu.interface /usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/modify Menu.mod


name=remove
description=Remove Global MS-DOS Programs
lininfo="/usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/remove \"Remove Global MS-DOS Programs\"" remove
action=OPEN MENU $INTFBASE/Menu.interface /usr/sadm/sysadm/add-ons/face/applmgmt/FACE/dos_programs/remove Menu.del

