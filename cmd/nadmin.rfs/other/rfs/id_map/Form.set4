	set -e GMAP="$F9";
	getfrm | set -l FRM;
	message "Group ID Mapping has been changed to $GMAP";
	"$INTFBASE/netservices/remote_files/other/rfs/id_map/bin/gid_map";
	`CLOSE $ARG1 $FRM
