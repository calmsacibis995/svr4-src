all : ALL

install: ALL 
	$(INS) -f  $(INSDIR) $(MAINS) ;\
	$(INS) -f  $(ROOT)/usr/lib/locale/C chrtbl_C ;\
	$(CH)./chrtbl chrtbl_C ;\
	$(CH)$(INS) -f $(ROOT)/usr/lib/locale/C LC_CTYPE ;\
	$(CH)$(INS) -f $(ROOT)/usr/lib/locale/C LC_NUMERIC ;\
	$(CH)rm -f LC_CTYPE LC_NUMERIC

