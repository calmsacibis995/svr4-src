MAKEFILE = chrtbl.mk


MAINS = chrtbl

OBJECTS =  chrtbl.o

SOURCES =  chrtbl.c

ALL:		$(MAINS)

chrtbl:		chrtbl.o 
	$(CC) $(CFLAGS)  -o chrtbl  chrtbl.o   $(LDFLAGS)


chrtbl.o:	 $(INC)/ctype.h $(INC)/signal.h \
		 $(INC)/stdio.h $(INC)/string.h \
		 $(INC)/sys/signal.h $(INC)/varargs.h 

GLOBALINCS = $(INC)/ctype.h $(INC)/signal.h $(INC)/stdio.h \
	$(INC)/string.h $(INC)/sys/signal.h \
	$(INC)/varargs.h 


clean:
	rm -f $(OBJECTS)

clobber:
	rm -f $(OBJECTS) $(MAINS)

newmakefile:
	makefile -m -f $(MAKEFILE)  -s INC $(INC)
