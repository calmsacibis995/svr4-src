#ident	"@(#)devintf:Prototype	1.1"

!MENU=usr/sadm/sysadm/menu
!INTFBIN=usr/sadm/sysadm/bin

## Files from local prototype: component=devintf

# foundation set (predecessor) directories

d foundation usr 0755 root sys
d foundation usr/sadm 0755 bin bin
d foundation usr/sadm/sysadm 0755 bin bin
d foundation $INTFBIN 0755 bin bin
d foundation $MENU 0755 bin bin

# foundation set files

f foundation $INTFBIN/mkdtab 0755 bin bin

# basic and extended OAM directories

d OAM_basic $MENU/devices 0755 bin bin
d OAM_basic $MENU/devices/devices 0755 bin bin
d OAM_basic $MENU/devices/devices/add 0755 bin bin
d OAM_basic $MENU/devices/devices/attrs 0755 bin bin
d OAM_basic $MENU/devices/devices/attrs/add 0755 bin bin
d OAM_basic $MENU/devices/devices/attrs/list 0755 bin bin
d OAM_basic $MENU/devices/devices/attrs/modify 0755 bin bin
d OAM_basic $MENU/devices/devices/attrs/remove 0755 bin bin
d OAM_basic $MENU/devices/devices/list 0755 bin bin
d OAM_basic $MENU/devices/devices/remove 0755 bin bin
d OAM_basic $MENU/devices/devices/reserve 0755 bin bin
d OAM_basic $MENU/devices/devices/reserve/free 0755 bin bin
d OAM_basic $MENU/devices/devices/reserve/list 0755 bin bin
d OAM_basic $MENU/devices/devices/reserve/reserve 0755 bin bin
d OAM_extended $MENU/devices/groups 0755 bin bin
d OAM_extended $MENU/devices/groups/add 0755 bin bin
d OAM_extended $MENU/devices/groups/list 0755 bin bin
d OAM_extended $MENU/devices/groups/mbrship 0755 bin bin
d OAM_extended $MENU/devices/groups/mbrship/add 0755 bin bin
d OAM_extended $MENU/devices/groups/mbrship/list 0755 bin bin
d OAM_extended $MENU/devices/groups/mbrship/remove 0755 bin bin
d OAM_extended $MENU/devices/groups/remove 0755 bin bin

# basic and extended interface

f OAM_ehelp $MENU/devices/devices/Help 0644 bin bin
f OAM_basic $MENU/devices/devices/add/Form.add 0644 bin bin
f OAM_ehelp $MENU/devices/devices/add/Help 0644 bin bin
f OAM_basic $MENU/devices/devices/add/Text.c_add 0644 bin bin
f OAM_ehelp $MENU/devices/devices/attrs/Help 0644 bin bin
