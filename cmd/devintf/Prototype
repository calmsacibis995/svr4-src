f OAM_basic $MENU/devices/devices/attrs/modify/Text.c_modify 0644 bin bin
f OAM_basic $MENU/devices/devices/attrs/remove/Form.remove 0644 bin bin
f OAM_ehelp $MENU/devices/devices/attrs/remove/Help 0644 bin bin
f OAM_basic $MENU/devices/devices/attrs/remove/Text.c_rem 0644 bin bin
f OAM_basic $MENU/devices/devices/devices.menu 0644 bin bin
f OAM_ehelp $MENU/devices/devices/list/Help 0644 bin bin
