case "$1" in

'start')
	[ -x /usr/pci/bin/pcistart ] && /usr/pci/bin/pcistart
	;;

'stop')
	[ -x /usr/pci/bin/pcistop ] && /usr/pci/bin/pcistop
	;;

esac

