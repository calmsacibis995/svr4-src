d OAM_basic $MENU/ports/port_services 0755 bin bin
d OAM_basic $MENU/ports/port_services/add 0755 bin bin
d OAM_basic $MENU/ports/port_services/disable 0755 bin bin
d OAM_basic $MENU/ports/port_services/enable 0755 bin bin
