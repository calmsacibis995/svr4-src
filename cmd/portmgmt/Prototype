#ident	"@(#)portmgmt:Prototype	1.1.1.1"

!MENU=usr/sadm/sysadm/menu
!INTFBIN=usr/sadm/sysadm/bin

## Files from local prototype: component=portmgmt

# foundation set (predecessor) directories

d foundation usr 0755 root sys
d foundation usr/sadm 0755 bin bin
d foundation usr/sadm/sysadm 0755 bin bin
d foundation $INTFBIN 0755 bin bin
d foundation $MENU 0755 bin bin

# basic and extended OAM directories

d OAM_basic $MENU/ports 0755 bin bin
d OAM_basic $MENU/ports/port_quick 0755 bin bin
d OAM_basic $MENU/ports/port_monitors 0755 bin bin
