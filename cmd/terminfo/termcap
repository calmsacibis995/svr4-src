	:al=5*\EE:dl=5*\ER:kd=^V:us=\EG8:ue=\EG0:so=\E(:se=\E):\
	:ma=^Vj^K^P^L :ti=\E):te=\E(:sg#0:ug#1:CO=\E.2:CF=\E.0:\
	:am:bs:ce=\ET:cm=\E=%+ %+ :cl=^Z:co#80:dc=\EW:ei=:\
	:ku=^K:kl=^H:kr=^L:k0=^A@\r:k1=^Aa\r:k2=^Ab\r:k3=^Ac\r:\
	:k4=^Ad\r:k5=^Ae\r:k6=^Af\r:k7=^Ag\rk8=^Ah\r:k9=^Ai\r:\
