!MOD=kdb
!CONFDIR=/etc/conf
!PACKDIR=$CONFDIR/pack.d/$MOD

i pkginfo
i postinstall
i preremove
i copyright
i depend

!default 0544 bin bin

d none /tmp/kdb		775	bin	bin
v none /tmp/kdb/Driver.o=$PACKDIR/Driver.o ? ? ?
v none /tmp/kdb/Master=$CONFDIR/mdevice.d/$MOD ? ? ?
v none /tmp/kdb/System=$CONFDIR/sdevice.d/$MOD ? ? ?
